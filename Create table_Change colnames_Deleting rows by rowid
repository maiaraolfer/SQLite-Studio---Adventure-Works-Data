-- creating a new table called SalesData2020_copy

CREATE TABLE SalesData2020_copy (
    OrderDate TEXT,
    StockDate TEXT,
    OrderNumber INTEGER,
    ProductKey INTEGER,
    CustomerKey INTEGER,
    TerritoryKey INTEGER,
    OrderLineItem INTEGER,
    OrderQuantity INTEGER
);
-- inserting the data from SalesData2020 into SalesData2020_copy
INSERT INTO SalesData2020_copy (
    OrderDate, StockDate, OrderNumber, ProductKey, 
    CustomerKey, TerritoryKey, OrderLineItem, OrderQuantity
)
SELECT
    column1, column2, column3, column4,
    column5, column6, column7, column8
FROM SalesData2020;

SELECT * FROM SalesData2020_copy LIMIT 10; -- checking if the data was copied

DELETE FROM SalesData2020_copy WHERE rowid = 1; -- deleting first row (rowid = 1) -- deleting first row
